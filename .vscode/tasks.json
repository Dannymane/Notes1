{
   "version": "2.0.0",
   "tasks": [
      {
         "label": "build",
         "command": "dotnet",
         "type": "process",
         "args": [
            "build",
            "${workspaceFolder}/Notes1.csproj",
            "/property:GenerateFullPaths=true",
            "/consoleloggerparameters:NoSummary"
         ],
         "problemMatcher": "$msCompile"
      },
      {
         "label": "publish",
         "command": "dotnet",
         "type": "process",
         "args": [
            "publish",
            "${workspaceFolder}/Notes1.csproj",
            "/property:GenerateFullPaths=true",
            "/consoleloggerparameters:NoSummary"
         ],
         "problemMatcher": "$msCompile"
      },
      {
         "label": "watch",
         "command": "dotnet",
         "type": "process",
         "args": [
            "watch",
            "run",
            "--project",
            "${workspaceFolder}/Notes1.csproj"
         ],
         "problemMatcher": "$msCompile"
      },
      {
         "type": "npm",
         "script": "start",
         "isBackground": true,
         "options": {
            "cwd": "${workspaceFolder}/Angular/Projects/sandbox-app/sandbox"
          },
         "problemMatcher": {
           "owner": "typescript",
           "pattern": "$tsc",
           "background": {
             "activeOnStart": true,
             "beginsPattern": {
               "regexp": "(.*?)"
             },
             "endsPattern": {
               "regexp": "bundle generation complete"
             }
           }
         }
       }
   ]
}